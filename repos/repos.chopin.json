[
  {
    "kernelSource": {
      "name": "android_kernel_samsung",
      "repo": "https://github.com/huangkaikaihuang/sanxingnote10",
      "branch": "main",
      "device": "d2xq",
      "defconfig": "d2xq_chn_open_defconfig"
    },
  "withKernelSU": true,
  "toolchains": [
    {
      "repo": "https://github.com/proprietary-stuff/llvm-arm-toolchain-ship-10.0",
      "branch": "master",
      "name": "proton-clang",
      "binPath": ["bin"]
    },
    {
      "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
      "name": "gcc",
      "binPath": ["bin"]
    }
  ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "proton-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-gnu-"
        "CONFIG_BUILD_ARM64_DT_OVERLAY":"y"
        "CONFIG_BUILD_ARM64_DT_OVERLAY":"y"
        "CLANG_TRIPLE":"aarch64-linux-gnu-"
        "DTC_EXT":"/tools/dtc"
        
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": false,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
